algoritmo "semnome"

var

num1,num2,soma:inteiro

inicio

      escreva("DIgite um numero: ")
      leia(num1)
      
      escreva("Digite outro numero: ")
      leia(num2)
      
      soma <- num1 + num2
      
      escreval("A soma entre ", num1, " +", num2, " =", soma)




fimalgoritmo
